{
  "private": true,
  "packageManager": "pnpm@8.6.2",
  "engines": {
    "pnpm": ">=8.0.0"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "node ./build.cjs",
    "dev": "node-dev --no-notify src/index.ts",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --write .",
    "generate": "buf generate && prettier --write src/proto",
    "lint": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "3.385.0",
    "@aws-sdk/client-ecs": "^3.385.0",
    "@bufbuild/connect": "^0.12.0",
    "@bufbuild/connect-node": "^0.12.0",
    "@bufbuild/protobuf": "^1.3.0",
    "abort-controller-x": "^0.4.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "execa": "^7.2.0",
    "fast-json-patch": "^3.1.1"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-connect-es": "^0.12.0",
    "@bufbuild/protoc-gen-es": "^1.3.0",
    "@types/node": "~20.4.8",
    "esbuild": "^0.18.19",
    "esbuild-node-externals": "^1.8.0",
    "esbuild-register": "^3.4.2",
    "node-dev": "^8.0.0",
    "prettier": "^3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-pkg": "^0.18.0",
    "typescript": "^5.1.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    }
  }
}
