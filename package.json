{
  "private": true,
  "packageManager": "pnpm@8.6.2",
  "engines": {
    "pnpm": ">=8.0.0"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "node ./build.cjs",
    "dev": "node-dev --no-notify src/index.ts",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --write .",
    "generate": "buf generate && prettier --write src/proto",
    "lint": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "3.357.0",
    "@aws-sdk/client-ecs": "^3.357.0",
    "@bufbuild/connect": "^0.10.1",
    "@bufbuild/connect-node": "^0.10.1",
    "@bufbuild/protobuf": "^1.2.1",
    "abort-controller-x": "^0.4.2",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "execa": "^7.1.1",
    "fast-json-patch": "^3.1.1"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-connect-es": "^0.10.1",
    "@bufbuild/protoc-gen-es": "^1.2.1",
    "@types/node": "~18.11.9",
    "esbuild": "^0.18.6",
    "esbuild-node-externals": "^1.7.0",
    "esbuild-register": "^3.4.2",
    "node-dev": "^8.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-pkg": "^0.17.1",
    "typescript": "^5.1.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    }
  }
}
